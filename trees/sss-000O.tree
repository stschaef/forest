\def\make-bib[topic]{
  % \query{
  %   \open\query
  %   \isect{\tag{\topic}}{\taxon{reference}}
  % }
  \query\datalog{
   ?X -:
     {\rel/has-tag ?X '{\topic}}
     {\rel/has-taxon ?X '{reference}}
   }
}

